<Script>-- Descheckeando todos los argumentos de lanzamiento
checksargumentos("descheck", "ar", "argumentos");

-- Desapareciendo y deshabilitando objetos varios
-- Primero los Inputs
local losinputs = {"co_elip", "co_elpuerto", "co_rutainit"};
for l in pairs(losinputs) do
	local velip = Input.IsVisible(losinputs[l]);
	if (velip == true) then
		Input.SetVisible(losinputs[l], false);
	end	
end
-- Ahora el Combox
local rcombox = ComboBox.GetSelected("co_resolucion");
if (rcombox ~= 1) then
	ComboBox.SetSelected("co_resolucion", 1);
	error = Application.GetLastError();
	if (error ~= 0) then
	    Dialog.Message("Error", "No fue posible seleccionar el primer item de la lista de resoluciones. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	end
end
local recombox = ComboBox.IsEnabled("co_resolucion");
if (recombox == true) then
	ComboBox.SetEnabled("co_resolucion", false);
	error = Application.GetLastError();
	if (error ~= 0) then
		Dialog.Message("Error", "No fue posible deshabilitar el objeto combox de la lista de resoluciones. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	else
		ComboBox.SetVisible("co_resolucion", false);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "Ocurrio un error desapareciendo el combox de las resoluciones. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	end
end		

-- los botonesHD
local losbohd = {"co_vm480", "co_vm720", "co_vm1080"};
for lpad in pairs(losbohd) do
	local elpades = Paragraph.IsVisible(losbohd[lpad]);
	if (elpades == true) then
		Paragraph.SetVisible(losbohd[lpad], false);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "Ocurrio un error desapareciendo un objeto paragraf llamado '"..losbohd[lpad].."'. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	end
end

-- Cosas del INIT
local cosasinit = {"co_autoinit", "co_editarinit", "co_ubicarinit"};
for ici in pairs(cosasinit) do
	-- primero los paragrafs
	local elpades = Paragraph.IsVisible(cosasinit[ici]);
	if (elpades == true) then
		Paragraph.SetVisible(cosasinit[ici], false);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "Ocurrio un error desapareciendo un objeto paragraf llamado '"..cosasinit[ici].."'. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	end
	-- ahora las imagenes asociadas a dichos paragrafs
	local elpades = Image.IsVisible(cosasinit[ici].."_img");
	if (elpades == true) then
		Image.SetVisible(cosasinit[ici].."_img", false);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "Ocurrio un error desapareciendo una imagen llamada '"..cosasinit[ici].."_img".."'. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	end
end


-- Habilitando los checks predefinidos.
local lospre = {"ar_console", "ar_screenshot", "ar_novideo"};
for l in pairs(lospre) do
    CheckBox.SetChecked(lospre[l], true);
    error = Application.GetLastError();
	if (error ~= 0) then
	    Dialog.Message("Error","Ocurrio un error dando check al objeto '"..lospre[l].."'. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	    Application.ExitScript();
	end
end	</Script>
