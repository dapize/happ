<Script>-- Muevo mi borde inferior, indicando q mi pestaña está activa
moverbordeabajo("ti_mo", "cab_borde_bajo"); 
-- Desaparesco otros objetos no relacionados con mi pestaña
dlgexapaodesa("mo");
-- Compruebo si se ha configurado la ruta del halo
local rutainput = Input.GetText("ar_ruta");
local rutapura = String.TrimLeft(String.TrimRight(rutainput, nil), nil);
if (rutapura ~= "") then
	rutacarpetahce = String.SplitPath(rutapura);
	-- Limpio las mods si por siacaso las alla ya enlistados
	local listaobjex = DialogEx.EnumerateObjects();
	for l in pairs(listaobjex) do
		local elobjeto = DialogEx.GetObjectType(listaobjex[l]);
		if (elobjeto == OBJECT_PARAGRAPH) then
			local propiobjex = Paragraph.GetProperties(listaobjex[l]);
			local preobjex = String.Right(propiobjex.ObjectName, 4);
			if (preobjex == "_mod") then
				DialogEx.DeleteObject(propiobjex.ObjectName);
			end
		end
	end
	-- Enlisto las modificaciones
	local nordenmod = 1;
	local lasmods = File.Find(_SourceFolder.."\\mods", "*.ini", false, false, nil, nil);
	for m in pairs(lasmods) do
		local rutapartidamod = String.SplitPath(lasmods[m]);
		-- Datos de la modificación:
		local ndarchivo = rutapartidamod.Filename; -- Nombre de archivo (1)
		local nombremod = INIFile.GetValue(_SourceFolder.."\\mods\\"..ndarchivo..".ini", "datos", "nombre"); -- Nombre completo (2)
		if (nombremod ~= "") then
			local webmod = INIFile.GetValue(_SourceFolder.."\\mods\\"..ndarchivo..".ini", "datos", "web"); -- Página web (3)
			if (webmod ~= "") then
				local descargamod = INIFile.GetValue(_SourceFolder.."\\mods\\"..ndarchivo..".ini", "datos", "descarga"); -- Enlace de descarga. (4)
				if (descargamod ~= "") then
					local tablaitems = INIFile.GetValueNames(_SourceFolder.."\\mods\\"..ndarchivo..".ini", "archivos"); -- Tabla de ruta de los archivos (5)
					if (tablaitems ~= nil) then
						citems = Table.Count(tablaitems); -- n° archivos
						-- Indices para la recorrida (Osea que se corre dos veces xD) 
						modactivado = 0;
						moddesactivado = 0;
						-- Recorro toda la lista de archivos
						for a = 1, citems do
							local partmod = INIFile.GetValue(_SourceFolder.."\\mods\\"..ndarchivo..".ini", "archivos", a);
							if (partmod ~= "") then
								-- Compruebo si está activado
								local rutaFile = reemplazoDeVariables(partmod);
								local partmodactivo = File.DoesExist(rutaFile[2]);
								if (partmodactivo == true) then
									modactivado = modactivado + 1;
								else
									-- Compruebo si está desactivado
									local archimodparto = String.SplitPath(rutaFile[2]);
									local partmoddesac = File.DoesExist(archimodparto.Drive..""..archimodparto.Folder..""..archimodparto.Filename..""..archimodparto.Extension..".happ");
									if (partmoddesac == true) then
										moddesactivado = moddesactivado + 1;
									end
								end
							else
								Dialog.Message("Espera", "No fué posible obtener la data necesaria para comprobar el mod.\Archivo del error: "..ndarchivo..".ini", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
								Application.ExitScript();
							end	
						end
						-- Compruebo que salio	
						if (modactivado == citems) then -- Está instalado y ACTIVADO
							creatituyboton(ndarchivo, nombremod, "Activado", webmod, descargamod, nordenmod);
						else
							if (moddesactivado == citems) then -- Está instalado pero DESACTIVADO
								creatituyboton(ndarchivo, nombremod, "Desactivado", webmod, descargamod, nordenmod);
							else -- No está instalado
								creatituyboton(ndarchivo, nombremod, "Instalar", webmod, descargamod, nordenmod);
							end	
						end
						-- Elimino variables comprobatorias de estado de la modificación.
						modactivado = nil;
						moddesactivado = nil;	
					end	
				end
			end
		end
		nordenmod = nordenmod + 1;
	end
	nordenmod = 1;
else
	Dialog.Message("Espera", "Aun no haz configurado la ruta del juego, tienes que hacerlo para determinar si las modificaciones están instaladas, activadas y/o desactivadas.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);	
	DialogEx.ClickObject("ti_ar");
	Application.ExitScript();
end</Script>
