function openUti(utiNumber)
	apagaboton2("uti"..utiNumber);
	if (Image.IsVisible("uti"..utiNumber.."_borrar")) then
		Image.SetVisible("uti"..utiNumber.."_borrar", false);
		Image.SetVisible("uti"..utiNumber.."_img", false);
		Paragraph.SetVisible("uti"..utiNumber, false);
		Paragraph.SetEnabled("uti"..utiNumber, false);
		Table.Insert(utilidadesPorBorrar, Table.Count(utilidadesPorBorrar) + 1, utiNumber);
		Application.ExitScript();
	end

	local theUti = INIFile.GetValue(_SourceFolder.."\\config.ini", "utilidades", "uti"..utiNumber);
	if(theUti ~= "") then
		local theFile = File.DoesExist(theUti);
		if (theFile) then
			local rutaUti = String.SplitPath(theUti);
			File.Run(theUti, "", rutaUti.Drive..""..rutaUti.Folder, SW_SHOWNORMAL, false);
			error = Application.GetLastError();
			if (error ~= 0) then
			    Dialog.Message("Error", "Ocurrió un error ejecutando la utilidad. Código de error: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
			end
		end
	else
		Dialog.Message("Awuanta :V", "No se pudo obtener la ruta guardada de la utilidad.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	end
end

function cargandoUtilidades()
	local uti = 1
	while uti &lt;= 10 do
		Image.SetVisible("uti"..uti.."_img", false);
		Paragraph.SetVisible("uti"..uti, false);
		uti = uti + 1;
	end

	local utilidades = INIFile.GetValueNames(_SourceFolder.."\\config.ini", "utilidades");
	if (utilidades ~= nil) then
		for uti in pairs(utilidades) do
			Paragraph.SetVisible(utilidades[uti], true);
			Paragraph.SetEnabled(utilidades[uti], true);
			local splitRutaUti = String.SplitPath(INIFile.GetValue(_SourceFolder.."\\config.ini", "utilidades", utilidades[uti]));
			Image.Load(utilidades[uti].."_img", _SourceFolder.."\\utilidades\\"..splitRutaUti.Filename..".png");
			Image.SetVisible(utilidades[uti].."_img", true);
		end
		Image.SetVisible("eliminar_img", true);
		Paragraph.SetVisible("eliminar", true);
	end
end

function borrados(show)
	-- ocultando el mismo boton de borrar
	if (show) then
		Paragraph.SetVisible("eliminar", false);
		Image.SetVisible("eliminar_img", false);
	else
		Paragraph.SetVisible("eliminar", true);
		Image.SetVisible("eliminar_img", true);
	end
	
	-- mostrando u ocultando los botones de acción
	Paragraph.SetVisible("aceptar", show);
	Image.SetVisible("aceptar_img", show);
	Paragraph.SetVisible("cancelar", show);
	Image.SetVisible("cancelar_img", show);
	
	-- mostrando u ocultando los botones de borrado
	local uti = 1
	while uti &lt;= 10 do
		local hisParagraph = Paragraph.IsVisible("uti"..uti);
		if(hisParagraph) then
			Image.SetVisible("uti"..uti.."_borrar", show);
		end
		uti = uti + 1;
	end
end

