-- Verificamos la correcta inserción del IP, puerto y clave, según corresponda
function veriipclave()
	local ipdelserver = Input.GetText("ip");
	local ippuro = String.TrimLeft(String.TrimRight(ipdelserver, nil), nil);
	local tomaip2 = String.Replace(ippuro, " ", "", true);
	local ipsolaa = String.Replace(tomaip2, "IPdelservidor", "", false);
	if (ipsolaa ~= "") then
		ipsola = ipsolaa; -- te da la IP Sola
		local puertobruto = Input.GetText("puerto");
		local puertobruto2 = String.TrimLeft(String.TrimRight(puertobruto, nil), nil);
		local puertobruto3 = String.Replace(puertobruto2, " ", "", true);
		local puertoa = String.Replace(puertobruto3, "Puerto", "", true);
		if (puertoa == "") then
			Dialog.Message("Espera!", "Te falta ingresar el puerto.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			Application.ExitScript();
			else
			puerto = puertoa; -- te da el puerto solo
			local tendraclave = Input.IsVisible("laclave");
			if (tendraclave == true) then
				local inpuclave = Input.GetText("laclave");
				local limpiclavea = String.TrimLeft(String.TrimRight(inpuclave, nil), nil);
				local limpiclaveb = String.Replace(limpiclavea, " ", "", true);
				if (limpiclaveb ~= "") then
					claveipdirecta = limpiclaveb; -- tira la clave si es que tiene claro
					else
					claveipdirecta = "";
				end
				else
				claveipdirecta = "";	
			end	
		end
		else
		Dialog.Message("Espera!", "Te falta ingresar el IP.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		Application.ExitScript();
	end
end

