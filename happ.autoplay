<Event>
<Name>On Click</Name>
<Args/>
<Script>function elexehce(larutaexe)
	if (larutaexe ~= "") then
		local existe = File.DoesExist(larutaexe.."\\haloce.exe"); -- verifico si existe haloce.exe
		if (existe == true) then
			Input.SetText("ar_ruta", larutaexe.."\\haloce.exe");
			error = Application.GetLastError();
			if (error ~= 0) then
    			Dialog.Message("Error", "Ocurrio un error al ingresar la ruta del juego en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
    			else
    			result = Dialog.Message("Notice", "!Ruta del juego localizada correctamente!.\n\nRuta: "..larutaexe.."\\haloce.exe", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			end
		else
			local existe = File.DoesExist(larutaexe.."\\halo.exe"); -- si no existe haloce.exe verifico si existe halo.exe
			if (existe == true) then
				Input.SetText("ar_ruta", larutaexe.."\\halo.exe");
				error = Application.GetLastError();
				if (error ~= 0) then
	    			Dialog.Message("Error", "Ocurrio un error al ingresar la ruta del juego en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	    			else
	    			result = Dialog.Message("Notice", "!Ruta del juego localizada correctamente!.\n\nRuta: "..larutaexe.."\\halo.exe", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				end
			else
				result = Dialog.Message("Noticia", "Se detecto una clave de registro del juego existente en el registro de windows, pero no se encontró ningún ejecutable válido en la ruta indicada, la cual es: '"..larutaexe.."'.\n\nTendrá que localizarla el ejecutable del juego manualmente.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);	
			end	
		end
	else
		result = Dialog.Message("Noticia", "No se encontró ninguna clave de registro válida para la auto localización del ejecutable del juego.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);	
	end	
end
local losbits = System.Is64BitOS();
if (losbits == true) then
	local regirutahce64 = Wow64.RegistryGetValue(HKEY_LOCAL_MACHINE, "SOFTWARE\\Wow6432Node\\Microsoft\\Microsoft Games\\Halo CE", "EXE Path", true, Wow64.KEY64);
	elexehce(regirutahce64);
else
	local regirutahce32 = Registry.GetValue(HKEY_LOCAL_MACHINE, "SOFTWARE\\Microsoft\\Microsoft Games\\Halo CE", "EXE Path", true);
	elexehce(regirutahce32);
end</Script>
