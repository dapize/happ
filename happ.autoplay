<Event>
<Name>On Show</Name>
<Args/>
<Script>moverbordeabajo("ti_ar", "cab_borde_bajo");
dlgexapaodesa("ar");

-- Insertando la ruta del Halo CE en el input correspondiente.
local ubicacionhce = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ejecutable", "ubicacion");
if (ubicacionhce ~= "") then
	Input.SetText("ar_ruta", ubicacionhce);
	error = Application.GetLastError();
	if (error ~= 0) then
	    Dialog.Message("Error","Ocurrio un error insertando la ruta del juego en el objeto input correspondiente. El codigo de error fue: ".. _tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	    Application.ExitScript();
	end
end

-- Rellenando Inputs: IP y puerto.
local inputses = {"elip", "elpuerto"};
for i in pairs(inputses) do
	local vinput = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", inputses[i]);
	if (vinput ~= "") then
		local sinput = Input.IsVisible("co_"..inputses[i]);
		if (sinput == false) then
			Input.SetVisible("co_"..inputses[i], true);
		end
		Input.SetText("co_"..inputses[i], vinput);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "Ocurrio un error insertando el valor correspondiente en el objeto input llamado 'co_"..inputses[i].."' El Codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	else
		local sinput = Input.IsVisible("co_"..inputses[i]);
		if (sinput == true) then
			Input.SetVisible("co_"..inputses[i], false);
		end	
	end
end


-- Checkeando o Descheckeando los argumentos de lanzamiento
checksargumentos("checkodescheck", "ar", "argumentos");


-- Trabajando con las resoluciones.
local miresolucion =  INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
if (miresolucion ~= "") then
	modoVideo(true, miresolucion)
else
	modoVideo(false)
end

botoneshdreso();
-- Cosas del INIt
cosasexec();

-- Pestaña de Modificiones
local losmods = File.Find(_SourceFolder.."\\mods", "*.ini", false, false, nil, nil);
if (losmods ~= nil) then
	-- activando pestaña de mods
	local pestamods = Label.IsVisible("ti_mo");
	if (pestamods == false) then
		Label.SetVisible("ti_mo", true);
		error = Application.GetLastError();
		if (error ~= 0) then
			Dialog.Message("Error", "No fue posible aparecer la pestaña de 'Modificaciones'. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		end
	end
end</Script>
