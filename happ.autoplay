<Name>On Click</Name>
<Args/>
<Script>btnOff("agregar");

local lasutiver = INIFile.GetValueNames(_SourceFolder.."\\config.ini", "utilidades");
if (lasutiver ~=  nil) then
	local nutils = Table.Count(lasutiver);
	if(nutils &gt;= 9) then
		result = Dialog.Message("Aviso", "Ya no es posible agregar más utilidades, ya no hay espacio, elimine algunas utilidades para agregar más.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);	
		Application.ExitScript();
	end
end

local localizacion = Dialog.FileBrowse(true, "Localiza el ejecutable de la utilidad", _DesktopFolder, "Executables (*.exe)|*.exe", "", "exe", false, false);
if (localizacion[1] ~= "CANCEL") and (localizacion ~= nil) then
	local lasUtilidades = INIFile.GetValueNames(_SourceFolder.."\\config.ini", "utilidades");
	if (lasUtilidades ~= nil) then
		for u in pairs(lasUtilidades) do
			local pathUti = INIFile.GetValue(_SourceFolder.."\\config.ini", "utilidades", lasUtilidades[u]);
			if (pathUti == localizacion[1]) then
				Dialog.Message("Espera!", "Yá haz agregado ésta utilidad.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				Application.ExitScript();
				break;
			end
		end
	end
	local utilisPath = _SourceFolder.."\\utilidades";
	local utilisFolder = File.DoesExist(utilisPath);
	if (utilisFolder ~= true) then
		Folder.Create(utilisPath);
		error = Application.GetLastError();
		if (error ~= 0) then
		    Dialog.Message("Error", "Ocurrió un error al crear la carpeta contenedora de los íconos de las utilidades.\n\nEl código de error fué: ".._tblErrorMessages[error]..".\n\nEs posible que este program necesite 'Ejecutarse como administrador', para poder realizar esta acción.", MB_OK, MB_ICONEXCLAMATION);
		    Application.ExitScript();
		end
	end

	local nombreutiexe = String.SplitPath(localizacion[1]);
	File.Run("AutoPlay\\Docs\\Quick_Any2Ico.exe", "\"-res="..localizacion[1].."\" \"-icon="..utilisPath.."\\"..nombreutiexe.Filename..".png\"", "", SW_SHOWNORMAL, false);
	error = Application.GetLastError();
    if (error ~= 0) then
        Dialog.Message("Error", "Ocurrió un error en el proceso de extracción del ícono a png del exe seleccionado para agregar como utilidad.\n\nEl código de error fué: ".._tblErrorMessages[error]..".\n\nEs posible que este programa necesite 'Ejecutarse como administrador', para completar correctamente esa acción.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
    else
    
    	local utilidades = INIFile.GetValueNames(_SourceFolder.."\\config.ini", "utilidades");
    	local nUtilidades
    	if (utilidades == nil) then
    		nUtilidades = 0
    	else
    		nUtilidades = Table.Count(utilidades);
    	end
    	
		local theUti = "uti"..(nUtilidades + 1)
		INIFile.SetValue(_SourceFolder.."\\config.ini", "utilidades", theUti, localizacion[1]);

		Paragraph.SetVisible(theUti, true);
		Paragraph.SetEnabled(theUti, true);
		
		Application.Sleep(500);

		Image.Load(theUti.."_img", utilisPath.."\\"..nombreutiexe.Filename..".png");
		Image.SetVisible(theUti.."_img", true);
		
		local botonEliminar = Paragraph.IsVisible("eliminar");
		if (botonEliminar ~= true) then
			Paragraph.SetVisible("eliminar", true);
			Image.SetVisible("eliminar_img", true);
		end

	end
end

</Script>
