<Name>On Click</Name>
<Args/>
<Script>
local haloceinput = Input.GetText("ar_ruta");
if (haloceinput == "") then
	Dialog.Message("¡Aviso!", "Tienes que localizar el ejecutable (haloce.exe o halo.exe) del juego.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
else
	-- obteniendo ruta del juego
	local rutahaloexe = String.TrimLeft(String.TrimRight(haloceinput, nil), nil);
	local existehalo = File.DoesExist(rutahaloexe);
	if (existehalo == false) then
		Dialog.Message("¡Espera!", "¡La ruta : '"..rutahaloexe.."' del juego configurada no es correcta!, por favor verificala manualmente y vuelvela a ingresar o localizar.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
   	else
   		-- Creando y guardando la ruta en "..nexeprogram.Filename..".ini
   		INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ejecutable", "ubicacion", rutahaloexe);
    	error = Application.GetLastError();
    	if (error ~= 0) then
        	Dialog.Message("Error", "Ocurrio un error al guardar las opciones, es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
        	DialogEx.Close(0);
       	else
        	local version = File.GetVersionInfo(rutahaloexe);
        	error = Application.GetLastError();
	    	if (error ~= 0) then
	        	Dialog.Message("Error", "Ocurrio un error al obtener 'la tabla de infomación de la versión' del ejecutable del juego, es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
	        	DialogEx.Close(0);
	        else
	        	INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ejecutable", "version", version.ProductVersion);	
	        	error = Application.GetLastError();
    			if (error ~= 0) then
        			Dialog.Message("Error", "Ocurrio un error al obtener la 'versión del producto' del ejecutable del juego, es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
        			DialogEx.Close(0);
        		end		
	        end
	         -- Guardando los Argumentos de lanzamiento
	        -- Guardando argumentos de lanzamiento con datos extras (inputs).
	     	checksespe = {"ip", "cport"};
	     	imputspe = {"elip",  "elpuerto"};
			for lces in pairs(checksespe) do
				local chespe = CheckBox.GetChecked("ar_"..checksespe[lces]);
				if (chespe == true) then
					local inpce = Input.GetText("co_"..imputspe[lces]);
					local inpcelim = String.Replace(inpce, " ", "", true);
					local inpcelimb = String.TrimLeft(String.TrimRight(inpcelim, nil), nil);
					if (inpcelimb ~= "") then
						INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", imputspe[lces], inpcelim);
						error = Application.GetLastError();
		    			if (error ~= 0) then
		        			Dialog.Message("Error", "Ocurrio un error al guardar el argumento de lanzamiento '"..checksespe[lces].."', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
		        			DialogEx.Close(0);	
		        		end
		        	else
		        		CheckBox.SetChecked("ar_"..checksespe[lces], false);	
		        		error = Application.GetLastError();
						if (error ~= 0) then
						    Dialog.Message("Error", "Ocurrió un error descheckeando el objeto checkbox llamado 'ar_"..checksespe[lces].."'. El error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						end
					end		
				else
					local hayaresp = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", imputspe[lces]);
					if (hayaresp ~= "") then
						INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", imputspe[lces]);
						error = Application.GetLastError();
		    			if (error ~= 0) then
		        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento '"..imputspe[lces].."', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
		        			DialogEx.Close(0);			
		        		else
		        			local haycipp = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", checksespe[lces]);
							if (haycipp ~= "") then
								INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", checksespe[lces]);
								error = Application.GetLastError();
				    			if (error ~= 0) then
				        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento '"..checksespe[lces].."', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
				        			DialogEx.Close(0);			
				        		end
				        	end	
		        		end
					end		
				end
			end
			-- Guardando la resolución
			local siresolucion = CheckBox.GetChecked("ar_vidmode");
			if (siresolucion == true) then
				local resoselect = ComboBox.GetSelected("co_resolucion");
				local resolucion = ComboBox.GetItemData("co_resolucion", resoselect);
				if (resolucion ~= "") then
					INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion", resolucion);
					error = Application.GetLastError();
    				if (error ~= 0) then
        				Dialog.Message("Error", "Ocurrio un error al guardar el argumento de lanzamiento 'Puerto', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
        				DialogEx.Close(0);
        			end
        		else
        			local hayresolucion = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
					if (hayresolucion ~= "") then
						INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
						error = Application.GetLastError();
		    			if (error ~= 0) then
		        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento 'Resolucion', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
		        			DialogEx.Close(0);
		        		end	
	        		end
				end
			else
				local hayresolucion = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
				if (hayresolucion ~= "") then
					INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
					error = Application.GetLastError();
	    			if (error ~= 0) then
	        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento 'Resolucion', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
	        			DialogEx.Close(0);
	        		end	
        		end
			end
	        -- Trabajando con INIT
			local elchecexec = CheckBox.GetChecked("ar_exec");
			if (elchecexec == true) then
				local larutainit = Input.GetText("co_rutainit");
				local larutainitp = String.TrimLeft(String.TrimRight(larutainit, nil), nil);
				if (larutainitp ~= "") then
					local archivoinit = File.DoesExist(larutainitp);
					if (archivoinit == false) then
						result = Dialog.Message("Error", "La ruta ingresada para cargar un init no es válida, es posible que no exista el archivo.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
						Application.ExitScript();
					else
						INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "init", larutainitp);
				    	error = Application.GetLastError();
				    	if (error ~= 0) then
				        	Dialog.Message("Error", "Ocurrio un error al guardar las opciones, es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
				       	end
					end
				else
					CheckBox.SetChecked("ar_exec", false);	
				end
			else
				local hayinit = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "init");
				if (hayinit ~= "") then
					INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "init");
					error = Application.GetLastError();
	    			if (error ~= 0) then
	        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento 'init', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
	        			DialogEx.Close(0);
	        		else
	        			local hayexec = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "exec");
						if (hayexec ~= "") then
		        			INIFile.DeleteValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "exec");
							error = Application.GetLastError();
			    			if (error ~= 0) then
			        			Dialog.Message("Error", "Ocurrio un error al eliminar el argumento de lanzamiento 'exec', es posible que este programa necesite 'Ejecutarce como administrador' para realizar esta acción.\n\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
			        			DialogEx.Close(0);
			        		end
			        	end		
	        		end	
        		end
			end
	     	checksargumentos("guardar", "ar", "argumentos"); -- ojo, lo llamo aquí al final, para que tome los checkbox deschekeados (si es el caso) por los comandos de arriba.
			btnOff("ar_guardar");
			result = DialogEx.Close(0);
   		end
	end	
end	</Script>
