function autolocalizarinit()
	local localizado = 0;
	local rutadirecthce = Input.GetText("ar_ruta");
	local rutadirectp = String.TrimLeft(String.TrimRight(rutadirecthce, nil), nil);
	if (rutadirectp ~= "") then
		local rutapartidahce = String.SplitPath(rutadirectp);
		local rutaDeInit = rutapartidahce.Drive..""..rutapartidahce.Folder.."\\init.txt";
		local autoinit = File.DoesExist(rutaDeInit);
		if (autoinit == true) then
			rutainitpura = rutaDeInit;
			result = Dialog.Message("¡Bien!", "!Ruta del INIT localizada correctamente!.\n\nRuta: "..rutapartidahce.Drive..""..rutapartidahce.Folder.."\init.txt", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			Input.SetText("co_rutainit", rutapartidahce.Drive..""..rutapartidahce.Folder.."\init.txt");
			error = Application.GetLastError();
			if (error ~= 0) then
			    Dialog.Message("Error", "Ocurrió un error ingresando la ruta del INIT localizada en el objeto input correspondiente. El codigo de error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
			else
			localizado = 1;	
			end
		else
			local crearinit = Dialog.Message("Pregunta", "No se localizó ningún archivo INIT, ¿deseas crear un archivo INIT automáticamente? .", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
			if (crearinit == IDYES) then
				local rutainput = Input.GetText("ar_ruta");
				local rutapura = String.TrimLeft(String.TrimRight(rutainput, nil), nil);
				if (rutapura ~= "") then
					local rutacarptea = String.SplitPath(rutapura);
					TextFile.WriteFromString(rutacarptea.Drive..""..rutacarptea.Folder.."\\init.txt", "", false);
					error = Application.GetLastError();
					if (error ~= 0) then
						Dialog.Message("Error", "Ocurrio un error creando el archivo init.txt, es posible que no se tenga permisos para escribir en el disco, pruebe 'ejecutando este programa como administrador'. El error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						Application.ExitScript();
					else
						Input.SetText("co_rutainit", rutacarptea.Drive..""..rutacarptea.Folder.."\init.txt");
					    error = Application.GetLastError();
						if (error ~= 0) then
							Dialog.Message("Error", "No fué posible ingresar la ruta del INIT en el objeto input indicado. El error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						else
							rutainitpura = rutacarptea.Drive..""..rutacarptea.Folder.."\\init.txt";
							localizado = 2;
						end
					end
				else
					result = Dialog.Message("¿Ozhe khá? XD", "No fué posible crear el archivo INIT, ya que aun no se a localizado la ruta del juego.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
				end	
			end
		end
	else
		result = Dialog.Message("¡Espera!", "No es posible auto localizar el archivo INIT, sin antes ingresar la ruta del juego primero.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	end
	return localizado;
end

