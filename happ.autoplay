<Args/>
<Script>local losparautis = {"editar", "eliminar", "agregar"}
for lpu in pairs(losparautis) do
	local visipara = Paragraph.IsVisible(losparautis[lpu]);
	if (visipara == true) then
		Paragraph.SetVisible(losparautis[lpu], false);
	else
		Paragraph.SetVisible(losparautis[lpu], true);
	end
	local visipara = Image.IsVisible(losparautis[lpu].."_img");
	if (visipara == true) then
		Image.SetVisible(losparautis[lpu].."_img", false);
	else
		Image.SetVisible(losparautis[lpu].."_img", true);
	end
end</Script>
