function actidesacmod(elmod)
	--larutadelhce
	local elbotonmod = Paragraph.GetProperties("mo_"..elmod.."_estado_mod");
	if (elbotonmod ~= nil) then
		local modInit = _SourceFolder.."\\mods\\"..elmod..".ini";
		local archivosmod = INIFile.GetValueNames(modInit, "archivos");
		if (archivosmod ~= nil) then
			if (elbotonmod.BorderColor == 3355443) then
				local enlaceDescarga = INIFile.GetValue(modInit, "datos", "descarga");
				if (enlaceDescarga == "") then
					Dialog.Message("Espera", "No existe el enlace de descarga del MOD.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
					Application.ExitScript();
				else
					local proParagraph = Paragraph.GetProperties("mo_"..elmod.."_estado_mod");
					error = Application.GetLastError();
					if (error ~= 0) then
					    Dialog.Message("Error", "Hubo un error obteniendo las propiedades del objeto 'mo_"..elmod.."_estado_mod'.\n\nEl código de error es: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						Application.ExitScript();
					end

					DialogEx.CreateObject(OBJECT_PROGRESS, "mo_"..elmod.."_estado_mod_progress", {FontSize = 9, Style = 1, Orientation = 0, Width = proParagraph.Width, Height = proParagraph.Height, X = proParagraph.X, Y = proParagraph.Y, XPStyle = false, MinRange = 0, MaxRange = 100, BarColor = 16750121, BackgroundColor = 0, TextColor = 16777215, UseCustomColors = true, Text = 'Descargando...'});
					error = Application.GetLastError();
					if (error ~= 0) then
					    Dialog.Message("Error", "Ocurrió un error creando el objeto de progreso.\n\nCódigo de error: ".._tblErrorMessages[error], MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
					    Application.ExitScript();
					end
						
					local splitEnlace = String.SplitPath(enlaceDescarga);
					local tempRutaZip = _TempFolder.."\\"..splitEnlace.Filename..""..splitEnlace.Extension;
					local nameObjMod = "mo_"..elmod.."_estado_mod";
					
					--[[
					local sessionNumber = Application.GetWndHandle();
					local descargaMod = Download.Start(sessionNumber, enlaceDescarga, tempRutaZip, "", "", false, nil, -1, nameObjMod, tempRutaZip);
					if (descargaMod == nil) then
						Dialog.Message("Error", "No fué posible iniciar la sesión de descarga.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
						Download.Delete(sessionNumber);
					end
					]]--
					
					
					local nameProgressObj = "mo_"..elmod.."_estado_mod_progress";
					local protocol = String.Mid(enlaceDescarga, 0, 6);
					if (protocol == 'https') then
						HTTP.DownloadSecure(enlaceDescarga, tempRutaZip, MODE_BINARY, 20, 443, nil, nil, descargandoMod);
					else
						HTTP.Download(enlaceDescarga, tempRutaZip, MODE_BINARY, 20, 80, nil, nil, descargandoMod);
					end
					error = Application.GetLastError();
					if error ~= 0 then
						DialogEx.DeleteObject(nameProgressObj);
						instalandoDesdeZip(nameProgressObj, "Ocurrió un error al descargar el MOD.\n\nEl código de error es: ".._tblErrorMessages[error]..".\n\n¿Desesas instalar el MOD desde un archivo ZIP?");
        			else
        				instalandoMod(nameProgressObj, tempRutaZip);
        			end
        			
        			
				end
			else
				local narchivosmod = Table.Count(archivosmod);
				local desactivados = 0;
				local activados = 0;
				local nombremod = INIFile.GetValue(modInit, "datos", "nombre");
				for a = 1, narchivosmod do
					local elarchivomod = INIFile.GetValue(modInit, "archivos", a);
					local reemplazando = reemplazoDeVariables(elarchivomod);
					if (reemplazando[1]) then
						elarchivomod = reemplazando[2];
					else
						elarchivomod = rutacarpetahce.Drive..""..rutacarpetahce.Folder..""..elarchivomod;
					end
					
					local tripearchivomod = String.SplitPath(elarchivomod);	
					local rutaExtensionHapp = tripearchivomod.Drive..""..tripearchivomod.Folder..""..tripearchivomod.Filename..""..tripearchivomod.Extension..".happ";
					local textoError = "Ocurrio un error al desactivar la mod '"..nombremod.."', especificamente al trabajar con el archivo '"..tripearchivomod.Filename..""..tripearchivomod.Extension.."', el cual se encuentra ubicado en '"..tripearchivomod.Folder.."'.";
					if (elbotonmod.BorderColor == 10311703) then -- si la mod está ACTIVADO, vamos a desactivarlo.
						File.Rename(elarchivomod, rutaExtensionHapp);
						error = Application.GetLastError();
						if (error ~= 0) then
							Dialog.Message("Error", textoError.."\n\nEl error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
							Application.ExitScript();
						else
							desactivados = desactivados + 1;	
						end
					elseif (elbotonmod.BorderColor == 0) then -- si la mod está DESACTIVADA, la activamos
						File.Rename(rutaExtensionHapp, elarchivomod);
						error = Application.GetLastError();
						if (error ~= 0) then
							Dialog.Message("Error", textoError.."\n\nEl error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
							Application.ExitScript();
						else
							activados = activados + 1;	
						end
					end	
				end
				
				if (desactivados == narchivosmod) then
					pintamosboton(elmod, "Desactivado");
				elseif (activados == narchivosmod) then
					pintamosboton(elmod, "Activado");
				end
				desactivados = nil;
				activados = nil;
			end	
			
		else
			Dialog.Message("Error", "No fué posible leer el archivo INI del MOD, trate de ejecutar la aplicación como 'Administrador'.", MB_OK, MB_ICONEXCLAMATION);
		end
	end	
end

