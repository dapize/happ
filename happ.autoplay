<HeightRatio>-1.000000</HeightRatio>
</Ratios>
<Event>
<Name>On Click</Name>
<Args/>
<Script>local inputinit = Input.GetText("co_rutainit");
rutainitpura = String.TrimLeft(String.TrimRight(inputinit, nil), nil);
if (rutainitpura ~= "") then
	local archivoinitr = File.DoesExist(rutainitpura);
	if (archivoinitr == true) then
		result = DialogEx.Show("editorinit", true, nil, nil);
	else
		result = Dialog.Message("Error", "No es posible editar el archivo INIT, ya que la ruta de éste no es válida.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	end
else
	local autolocainit = Dialog.Message("Pregunta", "No hay ruta ingresada de ningún archivo INIT para editar, ¿quieres auto localizarlo? .", MB_YESNO, MB_ICONINFORMATION, MB_DEFBUTTON1);
	if (autolocainit == 6) then
		local autolinit = autolocalizarinit();
		if (autolinit == 1) then -- si se encontró
			result = DialogEx.Show("editorinit", true, nil, nil);
		elseif (autolinit == 2) then -- no se encontró pero se aceptó crear uno automáticamente :)
			result = DialogEx.Show("editorinit", true, nil, nil);
		else
			result = Dialog.Message("Awuanta", "Algo pasó, no sé que fué, pero algo pasó y no sé como solucionarlo :'V", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			Application.ExitScript();
		end
	elseif (crearinit == IDNO) then
		result = Dialog.Message("Entonces...", "Tendrás que localizarlo manualmente.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	end
end</Script>
