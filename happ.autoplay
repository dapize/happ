-- Reune todos los argumentos de lanzamientos necesarios para abrir con ellos el juego.
function argumentos()
	--local nexeprogram = String.SplitPath(_SourceFilename);
	rutahaloce = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ejecutable", "ubicacion");
	if (rutahaloce == "") then
		Dialog.Message("Espera", "Aun no haz configurado la ruta del juego\nlocalizalá en la siguiente ventana....", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		DialogEx.Close(0);
		DialogEx.Show("configuraciones", true, nil, nil);
		Application.ExitScript();
		else
		local existehaloce = File.DoesExist(rutahaloce);
		if (existehaloce == true) then
			local listarg = INIFile.GetValueNames(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos");
			if (listarg ~= nil) then
				local losargulanza = {};
				for s in pairs(listarg) do
					local elarg = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", listarg[s]);
					if (elarg == "true") then
						if (listarg[s] == "ip") then
							local elip = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "elip");
							if (elip ~= "") then
								Table.Insert(losargulanza, Table.Count(losargulanza)+1, "-ip "..elip);		
							else
								Dialog.Message("Error", "Se seleccionó usar una IP personalizada, pero no se ha encontrado ninguna IP valida para esta acción, por favor, verifique la configuración de los 'Argumentos de lanzamiento'.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);	
								Application.ExitScript();
							end			
						elseif (listarg[s] == "cport") then
							local elpuerto = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "elpuerto");
							if (elpuerto ~= "") then
								Table.Insert(losargulanza, Table.Count(losargulanza)+1, "-cport "..elpuerto);		
							else
								Dialog.Message("Error", "Se seleccionó usar un puerto personalizado, pero no se ha encontrado ningun puerto valida para esta acción, por favor, verifique la configuración de los 'Argumentos de lanzamiento'.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);	
								Application.ExitScript();
							end	
						elseif (listarg[s] == "vidmode") then
							local laresolucion = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "resolucion");
							if (laresolucion ~= "") then
								Table.Insert(losargulanza, Table.Count(losargulanza)+1, "-vidmode "..laresolucion);		
							else
								Dialog.Message("Error", "Se seleccionó usar un resolucióna personalizada, pero no se ha encontrado ninguna resolución valida para esta acción, por favor, verifique la configuración de los 'Resolución e INIT'.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);	
								Application.ExitScript();
							end	
						elseif (listarg[s] == "exec") then
							local execarg = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "argumentos", "init");
							if (execarg ~= "") then
								Table.Insert(losargulanza, Table.Count(losargulanza)+1, "-exec "..execarg);		
							else
								Dialog.Message("Error", "Se seleccionó usar un archivo init, pero no se ha encontrado una ruta válida de dicho archivo para esta acción, por favor verifique la configuración de los 'Resolución e INIT'.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);	
								Application.ExitScript();
							end		
						else
							Table.Insert(losargulanza, Table.Count(losargulanza)+1, "-"..listarg[s]);			
						end
					end
				end
				argutotal = Table.Concat(losargulanza, " ", 1, TABLE_ALL); -- Argumentos de lanzamiento preparados, y listos para usar :)
				carpetrutahaloce(); -- obtengo la ruta y el folder limpio del Halo CE
			end		
		else
			Dialog.Message("¡Espera!", "!La ruta: '"..rutahaloce.."' no es correcta!, vé a 'Configuraciones' y localiza el juego otra vez.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
			DialogEx.Close(0);
			--DialogEx.Show("configuraciones", true, nil, nil);
			Application.ExitScript();
		end
	end	
end

-- Lanza el juego
function abrehalo()
	argumentos();
	local abriendo = File.Run(rutahaloce, argutotal, carpetparteuna.Drive..""..folderhalo, SW_SHOWNORMAL, false);
    error = Application.GetLastError();
    if (error ~= 0) then
	    Dialog.Message("¡Error de ejecución!", "Ocurrio un error ejecutando el juego,\npor favor intentalo otra vez.\n\nEl codigo de error fue el siguiente: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
	    Application.ExitScript();
    else
        Application.Minimize();
    end
end

-- Multiples acciones, relacionadas a los checks de los argumentos de lanzamiento del dialogo 'Configuraciones'
function checksargumentos(accion, prefijo, seccion)
	local objdlg = DialogEx.EnumerateObjects();
	for o in pairs(objdlg) do
		local objtipo = DialogEx.GetObjectType(objdlg[o]);
		if (objtipo == OBJECT_CHECKBOX) then
			local ncheck = CheckBox.GetProperties(objdlg[o]);
			local prex = String.Left(ncheck.ObjectName, 3);
			if (prex == prefijo.."_") then
				local argpuro = String.TrimLeft(ncheck.ObjectName, prefijo.."_");
				local argcheck = CheckBox.GetChecked(ncheck.ObjectName);
				-- ¿Qué acción realizamos?
				if (accion == "descheck") then
					if (argcheck == true) then -- si está con check lo deschekeamos.
						CheckBox.SetChecked(ncheck.ObjectName, false);
						error = Application.GetLastError();
						if (error ~= 0) then
				    		Dialog.Message("Error", "Ocurrio un error descheckeando al objeto check llamado '"..ncheck.ObjectName.."'. El codigo de error fue ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
				    		Application.ExitScript();
						end
					end
				elseif (accion == "guardar") then
					if (argcheck == true) then -- si está o no está con check, guardo su estado en el ini de cualquier forma
						local elargumento = INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", seccion, argpuro, "true");
						error = Application.GetLastError();
						if (error ~= 0) then
							Dialog.Message("Error", "Ocurrio un error guardando el estado del check '"..ncheck.ObjectName.."', no fue posible escribir en el disco, posiblemente tenga que 'Ejecutar este programa como administrador' para completar esta acción. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
							DialogEx.Close(0);
							Application.ExitScript();
						end	
					else		
						local elargumento = INIFile.SetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", seccion, argpuro, "false");
						error = Application.GetLastError();
						if (error ~= 0) then
							Dialog.Message("Error", "Ocurrio un error guardando el estado del check '"..ncheck.ObjectName.."', no fue posible escribir en el disco, posiblemente tenga que 'Ejecutar este programa como administrador' para completar esta acción. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
							DialogEx.Close(0);
						Application.ExitScript();
						end
					end
				elseif (accion == "checkodescheck") then
					local elargumento = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", seccion, argpuro);
					if (elargumento == "true") then
						CheckBox.SetChecked(ncheck.ObjectName, true);	
						error = Application.GetLastError();
						if (error ~= 0) then
						    Dialog.Message("Error", "Ocurrio un error dandole 'Check' al objeto check llamado '"..ncheck.ObjectName.."'. El codigo de error fue ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						    Application.ExitScript();
						end
					else
						CheckBox.SetChecked(ncheck.ObjectName, false);	
						error = Application.GetLastError();
