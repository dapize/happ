-- Para pegar el IP y el puerto en los inputs correspondientes
function pegaipdirecto()
	local tomoipypuerto = WinApi.ClipBoardGetText();
	local limpio = String.TrimLeft(String.TrimRight(tomoipypuerto, nil), nil);
	local buscapuntos = String.Find(limpio, ":", 1, true);
	if (buscapuntos ~= -1) then
		local filtroip = buscapuntos - "1";
		local ipsolo = String.Mid(limpio, 1, filtroip);
		Input.SetText("ip", ipsolo);
		error = Application.GetLastError();
		if (error ~= 0) then
		    Dialog.Message("Error", "Ocurrio un error al pegar el IP en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
		    Application.ExitScript();
		else
			local filtropuerto = buscapuntos + "1";
			local cuento = String.Length(limpio);
			local puertosolo = String.Mid(limpio, filtropuerto, cuento);
			Input.SetText("puerto", puertosolo);
			error = Application.GetLastError();
			if (error ~= 0) then
			    Dialog.Message("Error", "Ocurrio un error pegando el puerto en el input correspondiente. El Codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
			    Application.ExitScript();
			end
		end
		else
		Dialog.Message("No hay nada!", "No se a detectado un IP y puerto valido para ingresar, recuerde tomar el siguiente formato de copiado: 000.000.000.000:0000\n\nOsea, se debe copiar todo el ip junto con los dos puntos y el puerto, un ejemplo de copiado sería el siguiente IP y puerto:  192.168.1.22:2302", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		Application.ExitScript();
	end
end

-- Para mover el borde de abajo, indicativo de la pestaña activa.
