	Progress.SetCurrentPos(nameProgressObj, porcentaje);
end

function instalandoDesdeZip(nombreDelProgress, textoAMostrar)
	local ubicoZip = Dialog.Message("Espera", textoAMostrar, MB_YESNO, MB_ICONEXCLAMATION, MB_DEFBUTTON1);
	if (ubicoZip == IDYES) then
		local archivoZip = Dialog.FileBrowse(true, "Localiza el ZIP del mod", _DesktopFolder, "All Files (*.zip)|*.zip|", "", "zip", false, false);	
		if (archivoZip[1] ~= "CANCEL") and (archivoZip ~= nil) then
			desdeZip = true;
			instalandoMod(nombreDelProgress, archivoZip[1]);
			Application.ExitScript();
		else
			DialogEx.DeleteObject(nombreDelProgress);
			DialogEx.ClickObject("ti_mo");
			Application.ExitScript();
		end
	else
		DialogEx.DeleteObject(nombreDelProgress);
		DialogEx.ClickObject("ti_mo");
	end
end

function instalandoMod(nameObjProgress, rutaDelZip)
	local zipContent = Zip.GetContents(rutaDelZip, true);
	error = Application.GetLastError();
	if (error ~= 0) then
	    instalandoDesdeZip(nameObjProgress, "Ocurrió un error leyendo el archivo ZIP del MOD a instalar, es posible que esté corrupto, vuelve a descargarlo.\n\nEl código de error es: ".. _tblErrorMessages[error].."\n\nTambien puedes instalar el mod localizando el ZIP manualmente ¿deseas hacerlo?");
	    Application.ExitScript();
	end

	local nTable = Table.Count(zipContent);
	if(nTable == 0) then
		instalandoDesdeZip(nameObjProgress, "Al parecer el zip del MOD está dañado xq no se detecta ningún archivo en su interior, es posible que la descarga esté corrupta o estén actualizando el mod, vuelve a intentarlo dentro de unos minutos.\n\nTambien tienes la opción de instalar el mod localizando el zip localmente...\n\n¿deseas hacerlo?");
		Application.ExitScript();
	end
	Progress.SetProperties(nameObjProgress, {BarColor = 4597453});
	Progress.SetText(nameObjProgress, "Instalando...");
	local luaToLoad = false;
	for z, file in pairs(zipContent) do
		if (luaToLoad == false) then
			if (file == "init.lua") then
				luaToLoad = true;
			end
		end
		local theSlash = String.Right(file, 1);
		if (theSlash == "\\") then
			local stringConvertido = reemplazoDeVariables(file);
			if (stringConvertido[1]) then
				local rutaSinSlash = String.TrimRight(stringConvertido[2], "\\");
				local folderExist = Folder.DoesExist(rutaSinSlash);
				if (folderExist == false) then
					Folder.Create(rutaSinSlash);
					error = Application.GetLastError();
					if (error ~= 0) then
					    Dialog.Message("Error", "Ocurrió un error crear la carpeta en la ruta '"..rutaSinSlash.."', no se puede seguir con la instalación del mod. El código de error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
					    Application.ExitScript();
					end
				end	
			end
		else
			local pathReal = reemplazoDeVariables(file);
			if (pathReal[1]) then
				pathReal = pathReal[2];
			end
			pathReal = String.SplitPath(pathReal);
			nameProgressObj = nameObjProgress;
			Zip.Extract(rutaDelZip, {""..file..""}, pathReal.Drive..""..pathReal.Folder, false, false, "", ZIP_OVERWRITE_ALWAYS, extrayendoZip);
			error = Application.GetLastError();
	        if (error ~= 0) then
	            Dialog.Message("Error", "Ocurrió un error extrayendo el zip del mod, no se puede continuar con la instalación.\n\nEl código de error es: "..tblErrorMessages[error].."\n\nEs posible que necesites 'Ejecutar como administrador' esta aplicación.", MB_OK, MB_ICONEXCLAMATION);
	            DialogEx.DeleteObject(nameObjProgress);
	            DialogEx.ClickObject("ti_mo");
	            Application.ExitScript();
	        end
		end
	end
	if (desdeZip ~= true) then
		File.Delete(rutaDelZip, false, false, false, nil);
		if (Application.GetLastError() ~= 0) then
		    Dialog.Message("Error", "No fué posible borrar el archivo redisual de la instalación del mod en la carpeta temporal, descuida, no es importante..");
		end
	end
	DialogEx.DeleteObject(nameObjProgress);
	DialogEx.ClickObject("ti_mo");
end

function Download.OnProgress(tblData)
	if (tblData ~= nil) then
		local theData = tblData.Data;
		-- Progreso de descarga de MOD
		local esMod = String.Left(theData, 3);
		if (esMod == "mo_") then
			Progress.SetCurrentPos(theData.."_progress", String.ToNumber(tblData.Percent));
			Progress.SetText(theData.."_progress", tblData.Percent.."%");
		end
	end
end


function Download.OnComplete(tblData)
	Download.Delete(tblData.Session);
	result = Dialog.Message("Notice", "completado.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	-- descarga de un MOD
	local esMod = String.Left(tblData.Data, 3);
	if (esMod == "mo_") then
		DialogEx.DeleteObject(tblData.Data.."_progress");
		instalandoMod(tblData.Data.."_progress", tblData.ExData);
	end
end

function Download.OnError(tblData)
	Download.Delete(tblData.Session);
	local theData = tblData.Data;
	-- error de descargar... instalando desde ZIP
	local esMod = String.Left(theData, 3);
	result = Dialog.Message("Notice", "error papu :v.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	if (esMod == "mo_") then
		DialogEx.DeleteObject(tblData.Data.."_progress");
		instalandoDesdeZip(theData.."_progress", "Ocurrió un error al descargar el MOD.\n\nEl código de error es: "..tblData.ErrorCode..".\n\n¿Desesas instalar el MOD desde un archivo ZIP?");
	end
end


function descargandoMod(nDownloaded, nTotal, TransferRate, SecondLeft, SecondsLeftFormat, Message)
    local sDownloaded = String.GetFormattedSize(nDownloaded, FMTSIZE_AUTOMATIC, true);
