function resolutionSplit(laResolucion)
	local palito1 = String.Find(laResolucion, "|", 1, false);
	local reAncho = String.Left(laResolucion, (palito1 - 1)); -- ancho de pantalla
	local palito2 = String.Find(laResolucion, "|", (palito1 + 1), false);
	local reAlto = String.Mid(laResolucion, (palito1 + 1), ((palito2 - 1) - palito1)); -- alto de pantalla
	local palito3 = String.Find(laResolucion, "|", (palito2 + 1), false);
	local reBits = String.Mid(laResolucion, (palito2 + 1), ((palito3 - 1) - palito2)); -- bits de pantalla
	local reHz = String.Mid(laResolucion, (palito3 + 1), - 1); -- frecuencia
	local reNumber = String.ToNumber(reAncho..""..reAlto);
	return {ancho = reAncho, alto = reAlto, bits = reBits, hz = reHz, number = reNumber};
end


function tomarresoluciones(elcomboxrem, laResolucion)
	local resolutions = WinApi.EnumDisplaySettings(); -- todas las resoluciones tomadas
	local resolutionsNumbers = {};
	local resolutionsPure = {};
	-- Loop de limpiesa de la tabla
	local theLastResolution = null;
	for r in pairs(resolutions) do
		if (theLastResolution == null) then
			theLastResolution = resolutions[r];
		else
			local lastResolutionData = resolutionSplit(theLastResolution);
			local currentResolutionData = resolutionSplit(resolutions[r]);
			if (currentResolutionData.ancho..""..currentResolutionData.alto..""..currentResolutionData.bits == lastResolutionData.ancho..""..lastResolutionData.alto..""..lastResolutionData.bits) then
				if (currentResolutionData.hz &gt; lastResolutionData.hz) then
					theLastResolution = resolutions[r];
				end
			else
				Table.Insert(resolutionsPure, Table.Count(resolutionsPure) + 1, theLastResolution);
				Table.Insert(resolutionsNumbers, 1, String.ToNumber(lastResolutionData.ancho..""..lastResolutionData.alto));
				theLastResolution = resolutions[r];
			end
		end
	end
	-- Ordenando lista
	Table.Sort(resolutionsNumbers, nil);
	local resolutionsSorted = {};
	local breakForPrincipal = false;
	for e in pairs(resolutionsPure) do
		local resolution = resolutionSplit(resolutionsPure[e]);
		for s in pairs(resolutionsNumbers) do
			if (resolutionsNumbers[s] == resolution.number) then
				Table.Insert(resolutionsSorted, s, {text = resolution.ancho.." x "..resolution.alto.." | "..resolution.hz.." Hz", data = resolution.ancho..","..resolution.alto..","..resolution.hz});
			end
		end
		
	end
	-- Agregando al combox de resoluciones
	for o in pairs(resolutionsSorted) do
		local resolutionData = resolutionsSorted[o];
		local itmagreg = ComboBox.InsertItem(elcomboxrem, -1, resolutionData.text, resolutionData.data);
		if (itmagreg == -1) then
			error = Application.GetLastError();
			if (error ~= 0) then
				Dialog.Message("Error", "Ocurrio un error al agregar un item al combox de resoluciones.\nCodigo de error: ".._tblErrorMessages[error].."", MB_OK, MB_ICONEXCLAMATION);
			end
			break;
		end
	end
	-- auto selección demandada
	if (laResolucion ~= nil) then
		seleccombox(laResolucion);
	end
end

