<Script>btnOff("aceptar");
borrados(false);
local utisABorrar = Table.Count(utilidadesPorBorrar);
if (utisABorrar == 0) then
	Application.ExitScript();
end

for t in pairs(utilidadesPorBorrar) do
	local pathUti = INIFile.GetValue(_SourceFolder.."\\config.ini", "utilidades", "uti"..utilidadesPorBorrar[t]);
	local splitPathUti = String.SplitPath(pathUti);
	File.Delete(_SourceFolder.."\\utilidades\\"..splitPathUti.Filename..".png", false, false, false, nil);
	INIFile.DeleteValue(_SourceFolder.."\\config.ini", "utilidades", "uti"..utilidadesPorBorrar[t]);
end
local utisACargar = {};
local utisRestantes = INIFile.GetValueNames(_SourceFolder.."\\config.ini", "utilidades");
if (utisRestantes ~= nil) then
	for i in pairs(utisRestantes) do
		local pathUti = INIFile.GetValue(_SourceFolder.."\\config.ini", "utilidades", utisRestantes[i]);
		Table.Insert(utisACargar, Table.Count(utisACargar) + 1, pathUti);
	end
	INIFile.DeleteSection(_SourceFolder.."\\config.ini", "utilidades");
	for l in pairs(utisACargar) do
		INIFile.SetValue(_SourceFolder.."\\config.ini", "utilidades", "uti"..l, utisACargar[l]);
	end
	cargandoUtilidades();
else
	Paragraph.SetVisible("eliminar", false);
	Image.SetVisible("eliminar_img", false);
end
utilidadesPorBorrar = nil;


</Script>
