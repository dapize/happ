	-- Averiguo cuales son las pestañas existentes, más no con la cual trabajar.
	for x in pairs(objdlgx) do
		local lobjt = DialogEx.GetObjectType(objdlgx[x]);
		if (lobjt == OBJECT_LABEL) then
			local nobjx = Label.GetProperties(objdlgx[x]);
			local preobjt = String.Left(nobjx.ObjectName, 3);
			if (preobjt == "ti_") then
				local suobjt = String.Right(nobjx.ObjectName, 2);
				if (suobjt ~= tab) then
					-- Listo!, ya las reuni todas en una tabla (lostabs).
					Table.Insert(lostabs, Table.Count(lostabs) + 1, suobjt); 
				end
			end
		end
	end
	-- Aparesco y desaparesco objetos del dialogo, menos los correspondiente (tab) [y si están invisibles los hago aparecer].
	for t in pairs(lostabs) do -- Recorro la tabla de los prefijos de los tabs
