function reemplazoDeVariables(theString)
	local hcePath = Input.GetText("ar_ruta");
	if (hcePath == "") then
		Dialog.Message("¡Falta!", "Aun no haz configurado la ruta del juego, configuralá para instalar un MOD", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
		Application.ExitScript();
	end
	local splitHcePath = String.SplitPath(hcePath);
	local theGameFolder = splitHcePath.Drive..""..splitHcePath.Folder;
	theGameFolder = String.Mid(theGameFolder, 1, String.Length(theGameFolder) - 1);
	local amsGlobalVariables = {_GameFolder = theGameFolder , _DesktopFolder = _DesktopFolder, _DesktopFolderCommon = _DesktopFolderCommon, _ProgramFilesFolder = _ProgramFilesFolder, _SourceDrive = _SourceDrive, _SourceFolder = _SourceFolder, _SystemFolder = _SystemFolder, _TempFolder = _TempFolder, _WindowsFolder = _WindowsFolder, _DocumentsFolder = Shell.GetFolder(SHF_MYDOCUMENTS)};
	local convertido = false;
	for key, variable in pairs(amsGlobalVariables) do
		local searchVariable = String.Find(theString, key, 1, true);
		if (searchVariable ~= -1) then
			convertido = true;
			theString = String.Replace(theString, key, variable, true);
			break;
		end
	end
	return {convertido, theString};
end

