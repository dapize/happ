<Script>function formatorichtext()
	RichText.SetSelection("contenidoinit", 1, -1);
	error = Application.GetLastError();
	if (error ~= 0) then
		Dialog.Message("Error", "Ocurrió un error seleccionando el texto del objeto richtext. El codigo de error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	else
		RichText.SetSelectionFormat("contenidoinit", {Bold=false, TextColor=12632256, FaceName="Arial"}, false);
	end
end

local initleido = TextFile.ReadToString(rutainitpura);
if (initleido ~= "") then
	RichText.SetText("contenidoinit", initleido, false);
	error = Application.GetLastError();
	if (error ~= 0) then
	    Dialog.Message("Error", "Ocurrió un error al insertar el INIT leído en el objeto Richtext correspondiente. El codigo de error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	else
		formatorichtext();
	end
else
	RichText.SetText("contenidoinit", "", false);	
	error = Application.GetLastError();
	if (error ~= 0) then
		Dialog.Message("Error", "Ocurrió un error limpiando el objeto richtext contenedor del INIT. El codigo de error fué: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	else
		formatorichtext();
	end
end	
</Script>
