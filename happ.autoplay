<Script>DialogEx.SetFocus("ipfalso");
local gipdirecta = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ipdirecta", "ip");
if (gipdirecta ~= "") then
	Input.SetText("ip", gipdirecta);
	error = Application.GetLastError();
	if (error ~= 0) then
	    Dialog.Message("Error", "Ocurrio un error al insertar el IP en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
	else
	    local gpuerto = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ipdirecta", "puerto");
		if (gpuerto ~= "") then
			Input.SetText("puerto", gpuerto);
			error = Application.GetLastError();
			if (error ~= 0) then
			    Dialog.Message("Error", "Ocurrio un error al insertar el 'puerto' en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
			else
				-- Apareciendo y moviendo objetos, si hay clave guardada
				local gclave = INIFile.GetValue(_SourceFolder.."\\"..nexeprogram.Filename..".ini", "ipdirecta", "clave");
				if (gclave ~= "") then
					cambioagregarclave(156, true, 31);
					Input.SetText("laclave", gclave);
					error = Application.GetLastError();
					if (error ~= 0) then
					    Dialog.Message("Error", "Ocurrio un error ingresando la clave guardada en el input correspondiente. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
					else
					    Image.Load("agregar_clave_img", "AutoPlay\\Images\\Buttons\\quitar_clave-off.png");
						error = Application.GetLastError();
						if (error ~= 0) then
						    Dialog.Message("Error", "Ocurrio un error cargando la imagen indicativa para borrar la clave guardada. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
						end
					end
				else
					local estaclave = Input.IsVisible("laclave");
					if (estaclave == true) then
						cambioagregarclave(125, false, -31);
					end
					Image.Load("agregar_clave_img", "AutoPlay\\Images\\Buttons\\agregar_clave-off.png");
					error = Application.GetLastError();
					if (error ~= 0) then
						Dialog.Message("Error", "Ocurrio un error cargando la imagen indicativa para agregar la clave guardada. El codigo de error fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
					end		
				end
				-- Apareciendo y desapareciendo el boton correspondiente de 'Guardar'
				Image.SetVisible("guardado_img", true);
				error = Application.GetLastError();
				if (error ~= 0) then
					Dialog.Message("Error", "Ocurrio un error apareciendo la imagen secundaria boton guardar. El codigo de erro fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
				else
					Image.SetVisible("guardar_img", false);
					error = Application.GetLastError();
					if (error ~= 0) then
						Dialog.Message("Error", "Ocurrio un error desapareciendo la imagen original del boton guardar. El codigo de erro fue: ".._tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
					else
						DialogEx.SetRedraw(true); 
					end
				end	
			end
		end
	end
end</Script>
